<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="750.0" style="-fx-background-color: #FFFFFF;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.EditController">
    <children>
        <TextField fx:id="Target" layoutX="144.0" layoutY="85.0" prefHeight="61.0" prefWidth="246.0"
                   promptText="A Word In Dict..." AnchorPane.leftAnchor="144.0" AnchorPane.rightAnchor="210.0">
            <cursor>
                <Cursor fx:constant="TEXT"/>
            </cursor>
        </TextField>
        <Text fill="#dd6262" layoutX="45.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Edit:"
              wrappingWidth="73.0">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <Text fill="#dd6262" layoutX="51.0" layoutY="202.0" strokeType="OUTSIDE" strokeWidth="0.0" text="By:"
              wrappingWidth="60.75390625">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <TextField fx:id="newTarget" layoutX="146.0" layoutY="215.0" prefHeight="61.0" prefWidth="246.0"
                   promptText="Another English Word..." AnchorPane.leftAnchor="144.0" AnchorPane.rightAnchor="210.0">
            <cursor>
                <Cursor fx:constant="TEXT"/>
            </cursor>
        </TextField>
        <Button fx:id="changeButton" layoutX="49.0" layoutY="338.0" mnemonicParsing="false" onAction="#changeWord"
                text="Change!" textAlignment="CENTER" textFill="#6f3030" textOverrun="LEADING_WORD_ELLIPSIS"
                AnchorPane.bottomAnchor="36.0" AnchorPane.leftAnchor="49.0">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <TextField fx:id="newExplain" layoutX="149.0" layoutY="313.0" prefHeight="61.0" prefWidth="246.0"
                   promptText="Vietnamese..." AnchorPane.leftAnchor="144.0" AnchorPane.rightAnchor="210.0">
            <cursor>
                <Cursor fx:constant="TEXT"/>
            </cursor>
        </TextField>
    </children>
</AnchorPane>
